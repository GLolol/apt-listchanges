#: ../apt-listchanges:109
msgid "%s: will be newly installed"
msgstr "%s on uusi asennus"

#: ../apt-listchanges:121
msgid "%s: Version %s has already been seen"
msgstr "Paketin %s versio %s on nähty aiemmin"

#: ../apt-listchanges:147
msgid "Changes for %s"
msgstr "Muutokset pakettiin %s"

#: ../apt-listchanges:156
msgid "Informational notes"
msgstr "Tiedotukset"

#: ../apt-listchanges:177
msgid "didn't find any valid .deb archives"
msgstr "en löytänyt kelvollisia .deb-arkistoja"

#: ../apt_listchanges.py:184
msgid ""
"Wrong or missing VERSION from apt pipeline\n"
"(is Dpkg::Tools::Options::/usr/bin/apt-listchanges::Version set to 2?)\n"
msgstr ""
"Väärä tai puuttuva VERSION putkessa aptilta\n"
"(onko Dpkg::Tools::Options::/usr/bin/apt-listchanges::Version arvoltaan 2?)\n"

#: ../apt_listchanges.py:225
msgid "Mailing changelogs to %s"
msgstr "Postitan muutoslokit osoitteeseen %s"

#: ../apt_listchanges.py:228
msgid "apt-listchanges output for %s"
msgstr "apt-listchangesin tulostus koneelta %s"

#: ../apt_listchanges.py:239
msgid "The newt frontend is deprecated, using pager"
msgstr "Newt-liittymä on vanhentunut, käytän sivutinta"

#: ../apt_listchanges.py:266
msgid "Do you want to continue? [Y/n]? "
msgstr "Haluatko jatkaa? [Kyllä=y/ei=n]? "

#: ../apt_listchanges.py:275 ../apt_listchanges.py:301
#: ../apt_listchanges.py:306
msgid "Reading changelogs"
msgstr "Luen muutoslokeja"

#: ../apt_listchanges.py:306
msgid "Done"
msgstr "Valmis"

#: ../apt_listchanges.py:343
msgid "%s exited with status %d"
msgstr "%s lopetti suorituksen palauttaen arvon %d"

