#: apt-listchanges:146
msgid "Must specify either --apt or filenames to process!"
msgstr "¡Tienes que especificar --apt o bien paquetes a procesar!"

#: apt-listchanges:165
#, c-format
msgid "Unable to determine package name for file %s"
msgstr "No se puede determinar el nombre del paquete para el fichero %s"

#: apt-listchanges:171
#, c-format
msgid "Unable to determine version for file %s"
msgstr "No se puede determinar la versión para el fichero %s"

#: apt-listchanges:185
#, c-format
msgid "%s: will be newly installed"
msgstr "%s: se instalará por primera vez"

#: apt-listchanges:193
#, c-format
msgid "%s: Version %s is already installed"
msgstr "%s: La versión %s ya está instalada"

#: apt-listchanges:200 apt-listchanges:216
#, c-format
msgid "%s: Version %s changes have already been seen"
msgstr "%s: La versión %s ya está instalada"

#: apt-listchanges:209
#, c-format
msgid "%s: Version %s is older than installed version (%s)"
msgstr "%s: La versión %s es más antigua que la versión instalada (%s)"

#: apt-listchanges:328
#, c-format
msgid "Changes for %s"
msgstr "Cambios para %s"

#: apt-listchanges:340
msgid "Informational notes"
msgstr "Notas informativas"

#: apt-listchanges:344
msgid "Error output follows"
msgstr "La salida de error sigue"

#: apt-listchanges:388
msgid "In order to use the newt frontend, you must install libnewt-perl"
msgstr "Para usar el frontend de newt, necesitas instalar libnewt-perl"

#: apt-listchanges:401
msgid "Reading changelogs"
msgstr "Leyendo changelogs"

#: apt-listchanges:406
msgid "Scanning packages..."
msgstr "Escaneando paquete..."

#: apt-listchanges:435
msgid "Displaying changelogs"
msgstr "Mostrando changelogs"

#: apt-listchanges:476
#, c-format
msgid "Reading changelogs...%s"
msgstr "Leyendo changelogs...%s"

#: apt-listchanges:477
msgid "Done"
msgstr "Hecho"

#: apt-listchanges:505
msgid "Unable to open tty for confirmation, assuming yes"
msgstr "No se puede abrir la tty para pedir confirmación, asumiendo sí"

#: apt-listchanges:513
msgid "Do you want to continue [Y/n]? "
msgstr "Quieres continuar? [Y/n]"

#: apt-listchanges:538
#, c-format
msgid "Unable to open temporary file %s: %s"
msgstr "No se puede abrir archivo temporal %s: %s"

#: apt-listchanges:614
#, c-format
msgid "Changelogs mailed to %s"
msgstr "Changelogs enviados a %s"

#: apt-listchanges:701
msgid "Wrong or missing VERSION from apt pipeline"
msgstr "VERSION errónea o inexistente proviniente de la tubería de apt"

#: apt-listchanges:702
msgid "(is Dpkg::Tools::Options::/usr/bin/apt-listchanges::Version set to 2?)"
msgstr "(¿está Dpkg::Tools::Options::/usr/bin/apt-listchanges::Version a 2?)"
